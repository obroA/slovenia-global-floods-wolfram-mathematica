Cell[CellGroupData[{

Cell["\<\
Calculate Overall Average Number of Floods per Year in Slovenia (1750-2010)\
\>", "Subsubsection",
 CellChangeTimes->{{3.9340057373249884`*^9, 3.934005795381825*^9}, 
   3.9340069054901605`*^9},ExpressionUUID->"3415caae-8291-4a93-b535-\
689a80bcc6d8"],

Cell["\<\
This function calculates the average number of floods per year across the \
entire time period from 1750 to 2010 in Slovenia. It provides insight into \
the general frequency of floods over the given historical range. The average \
is computed by dividing the total number of floods by the total number of \
years represented in the dataset.
Defining the function to calculate average number of floods per year in \
Slovenia.\
\>", "Text",
 CellChangeTimes->{{3.9340058200315065`*^9, 3.934005883343337*^9}, {
  3.9340069332390604`*^9, 
  3.934006939422429*^9}},ExpressionUUID->"fda91428-29d7-42c2-8eef-\
0d0c28e1494b"],

Cell[BoxData[
 RowBox[{
  RowBox[{"averageFloodsPerYear", "[", "data_List", "]"}], ":=", 
  RowBox[{"Module", "[", 
   RowBox[{
    RowBox[{"{", 
     RowBox[{"totalFloods", ",", "totalYears"}], "}"}], ",", 
    RowBox[{
     RowBox[{"totalFloods", "=", 
      RowBox[{"Total", "[", 
       RowBox[{"data", "[", 
        RowBox[{"[", 
         RowBox[{"All", ",", "3"}], "]"}], "]"}], "]"}]}], ";", 
     "\[IndentingNewLine]", 
     RowBox[{"totalYears", "=", 
      RowBox[{"Total", "[", 
       RowBox[{"(", 
        RowBox[{
         RowBox[{"data", "[", 
          RowBox[{"[", 
           RowBox[{"All", ",", "2"}], "]"}], "]"}], "-", 
         RowBox[{"data", "[", 
          RowBox[{"[", 
           RowBox[{"All", ",", "1"}], "]"}], "]"}], "+", "1"}], ")"}], 
       "]"}]}], ";", "\[IndentingNewLine]", 
     RowBox[{"N", "[", 
      RowBox[{"totalFloods", "/", "totalYears"}], "]"}]}]}], "]"}]}]], "Input",
 CellLabel->"In[50]:=",ExpressionUUID->"4451a9ea-ac35-424f-b098-f194242da526"],

